<div class="container">
  <thead>
    <h2 class="page-header">ジャンル一覧・追加</h2>
    <div class="row">
      <%= render 'layouts/error', obj: @genre %>
    </div>
    <div class="row form-group">
    <%= form_with model:[:admin,@genre],local: true do |f| %>
      <tr>
        <th>ジャンル名</th>
        <th><%= f.text_field :name %></th>
        <th><%= f.submit '新規登録', class: 'btn btn-success' %></th>
      </tr>
      <% end %>
    </div>
  </thead>
</div>